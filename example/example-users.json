{
	"get_users": [
		{
			"?status": "@user_status"
		},
		{
			"users": [{
				"id": ["uint8"],
				"profile": "@user_profile",
				"status": "@user_status"
			}]
		}
	],
	"get_user": [
		{
			"user_id": ["uint8"]
		},
		{
			"profile": "@user_profile",
			"created": "datetime",
			"sessions": ["@user_session"],
			"?social": {
				"?facebook_id": "string",
				"?twitter_id": "string",
				"?instagram_id": "string"
			}
		}
	],
	"delete_user": [
		{
			"user_id": ["uint8"]
		},
		{
			"?error": "string"
		}
	]
}